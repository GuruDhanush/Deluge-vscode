{
	"For Loop": {
		"prefix": [
			"for"
		],
		"body": [
			"for each ${2:item} in ${1:list} {",
			"\t$0",
			"}"
		],
		"description": "For loop"
	},
	"For index Loop": {
		"prefix": [
			"forindex"
		],
		"body": [
			"for each index ${2:indexName} in ${1:list} {",
			"\t$0",
			"}"
		],
		"description": "For loop with indexes"
	},
	"If": {
		"prefix": [
			"if"
		],
		"body": [
			"if(${1:test}) {",
			"\t$0",
			"}"
		],
		"description": "If statement"
	},
	"If else": {
		"prefix": [
			"ifelse"
		],
		"body": [
			"if(${1:test}) {",
			"\t$0",
			"} else {",
			"\t",
			"}"
		],
		"description": "If else"
	},
	"If conditional": {
		"prefix": [
			"ifcon"
		],
		"body": [
			"if(${1:test}, ${2:expression1}, ${3:expression2})$0"
		],
		"description": "if test is true, returns expression1 otherwise returns expression2"
	},
	"If Null": {
		"prefix": [
			"ifnull"
		],
		"body": [
			"ifnull(${1:expression1}, ${2:expression2})$0"
		],
		"description": "if expression1 is true, returns expression1 otherwise returns expression2"
	},
	"invokeurl": {
		"prefix": [
			"invokeurl"
		],
		"body": [
			"invokeurl [",
			"\turl: ${1:url}",
			"\ttype: ${2|GET,POST,PUT,DELETE|}",
			"\tparameters: ${3:parameters}",
			"\t];$0"
		],
		"description": "invoke url calls a url"
	},
	"sendemail": {
		"prefix": [
			"email",
			"sendemail"
		],
		"body": [
			"sendemail [",
			"\tfrom: ${1:fromEmail}",
			"\tto: ${2:toEmail}",
			"\tsubject: ${3:subject}",
			"\tmessage: ${4:message}",
			"\t];$0"
		],
		"description": "sends an email"
	},
	"sendsms": {
		"prefix": [
			"sms",
			"sendsms"
		],
		"body": [
			"sendsms [",
			"\tto: ${1:phoneNumber}",
			"\tmessage: ${2:message}",
			"\t];$0"
		],
		"description": "send an sms"
	},
	"pushNotification": {
		"prefix": [
			"notification",
			"pushNotification"
		],
		"body": [
			"pushNotification [",
			"\tto: ${1:email}",
			"\tmessage: ${2:message}",
			"\t];$0"
		],
		"description": "send an push notification"
	}
}